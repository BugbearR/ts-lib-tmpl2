{
  "jsc": {
    "parser": {
      "syntax": "typescript"
    },
    "target": "es5",
    "minify": {
      "compress": {
        "unused": true
      },
      "mangle": true
    }
  },
  "env": {
    "targets": {
      "ie": "11"
    },
    "mode": "usage",
    "coreJs": 3
  },
  "module": {
    "type": "umd"
  },
  "minify": true
}
